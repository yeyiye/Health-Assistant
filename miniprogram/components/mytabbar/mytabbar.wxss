/* components/mytabbar/mytabbar.wxss */
.tabBar{
  background-color: #FFF;
  position: fixed;
  bottom: 0;
  width: 100%;
  font-size: 10px;
  font-weight: 400;
  text-align: center;
  padding-top: 5px;
  border-top: 1px solid #E4E4E4;
  }
  
  .tabBar view{
  display: inline-block;
  }
  .tabBar view image {
  height: 25px;
  width: 25px;
  display: block;
  margin: 0 auto;
  }
  .tabBar view text {
  display: block;
  }